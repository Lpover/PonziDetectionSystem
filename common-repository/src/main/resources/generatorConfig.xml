<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <properties resource="connection.yml"/>
    <context id="context" targetRuntime="MyBatis3DynamicSql">
        <property name="autoDelimitKeywords" value="true" />
        <property name="beginningDelimiter" value="`" />
        <property name="endingDelimiter" value="`" />
        <plugin type="org.mybatis.generator.plugins.ToStringPlugin"/>
        <plugin type="org.mybatis.generator.plugins.FluentBuilderMethodsPlugin"/>
        <commentGenerator>
            <property name="suppressAllComments" value="true"/>
            <property name="addRemarkComments" value="true"/>
        </commentGenerator>
        <jdbcConnection connectionURL="${url}" driverClass="${driver}" userId="${user}"
                        password="${password}">
            <property name="nullCatalogMeansCurrent" value="true"/>
        </jdbcConnection>
        <javaModelGenerator targetPackage="cn.qkl.common.repository.model"
                            targetProject="src/main/java"/>
        <javaClientGenerator targetPackage="cn.qkl.common.repository.mapper"
                             targetProject="src/main/java"/>
        <table tableName="account" domainObjectName="Account">
        </table>
        <table tableName="account_check_history" domainObjectName="AccountCheckHistory">
        </table>
        <table tableName="account_content" domainObjectName="AccountContent">
        </table>
        <table tableName="account_tx_history" domainObjectName="AccountTxHistory">
        </table>
        <table tableName="algorithm" domainObjectName="Algorithm">
        </table>
        <table tableName="chain" domainObjectName="Chain">
        </table>
        <table tableName="content" domainObjectName="Content">
        </table>
        <table tableName="content_risk" domainObjectName="ContentRisk">
        </table>
        <table tableName="content_cross" domainObjectName="ContentCross">
        </table>
        <table tableName="content_tx_history" domainObjectName="ContentTxHistory">
        </table>
        <table tableName="dynamic_monitor" domainObjectName="DynamicMonitor">
        </table>
        <table tableName="platform" domainObjectName="Platform">
        </table>
        <table tableName="risk_content_statistics" domainObjectName="RiskContentStatistics">
        </table>
        <table tableName="risk_report" domainObjectName="RiskReport">
        </table>
        <table tableName="statistic" domainObjectName="Statistic">
        </table>
        <table tableName="content_risk_statistics" domainObjectName="ContentRiskStatistics">
        </table>
        <table tableName="user" domainObjectName="User">
        </table>

    </context>
</generatorConfiguration>